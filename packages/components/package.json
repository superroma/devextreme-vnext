{
  "name": "@devextreme-vnext/components",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --clean && rm -f tsconfig.tsbuildinfo && tsc -p tsconfig.json --emitDeclarationOnly --outDir dist"
  },
  "dependencies": {
    "@devextreme-vnext/core": "0.1.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.4.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
